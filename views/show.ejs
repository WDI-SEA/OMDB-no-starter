<h1><%=movieData.Title%></h1>
<img src= '<%=movieData.Poster%>' alt='Movie Poster'>

<h3>Release Date</h3>
<p><%=movieData.Released%></p>

<h3>Director</h3>
<p><%=movieData.Director%></p>

<h3>Stars</h3>
<p><%=movieData.Actors%></p>

<h3>Plot</h3>
<p><%=movieData.Plot%></p>

<% movieData.Ratings.forEach(rating => { %>
    <h3><%=rating.Source%> Rating</h3>
    <p><%=rating.Value%></p>
<%})%>

